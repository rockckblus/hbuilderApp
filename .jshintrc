{
    "boss": true,//缺少分号选项
    "curly": true,//循环或者条件语句必须使用花括号包围
    "eqnull": true,
    "expr": true,
    "immed": true,
    "newcap": true,
    "noempty": true,
    "noarg": true,//禁止单引号双眼引号混用
    "undef": true,//变量未使用
    "regexp": true,

    "browser": true,//预定义全局变量 document
    "devel": true,//定义用于调试的全局变量： console ， alert
    "jquery": true,

    "globals": {//全局忽略
        "define": false,
        "plus": false,
        "mui": false,
        "angular": false,
        "moment": false
    }
}